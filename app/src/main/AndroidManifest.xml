<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.beautyshop">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".TutorialActivity"></activity>

        <activity android:name=".UploadReviewActivity"
            android:parentActivityName=".ReviewActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".ReviewActivity"/>
        </activity>

        <activity android:name=".ReviewActivity"
            android:parentActivityName=".HomeActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".HomeActivity"/>
        </activity>

        <activity android:name=".DetailBeliActivity"
            android:parentActivityName=".BeliActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".BeliActivity"/>
        </activity>

        <activity android:name=".ProfilActivity"
            android:parentActivityName=".HomeActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".HomeActivity"/>
        </activity>


        <activity android:name=".BeliActivity"
            android:parentActivityName=".ProdukActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".ProdukActivity"/>
        </activity>

        <activity android:name=".ProdukActivity"
            android:parentActivityName=".ShopActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".ShopActivity"/>
        </activity>

        <activity android:name=".PixyActivity"
            android:parentActivityName=".ShopActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".ShopActivity"/>
        </activity>

        <activity android:name=".MadameGieActivity"
            android:parentActivityName=".ShopActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".ShopActivity"/>
        </activity>

        <activity android:name=".EminaActivity"
            android:parentActivityName=".ShopActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".ShopActivity"/>
        </activity>

        <activity android:name=".WardahActivity"
            android:parentActivityName=".ShopActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".ShopActivity"/>
        </activity>

        <activity android:name=".ShopActivity"
            android:parentActivityName=".HomeActivity">
        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value=".HomeActivity"/>
        </activity>

        <activity android:name=".HomeActivity"
            android:parentActivityName=".LoginActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".LoginActivity"/>
        </activity>

        <activity android:name=".MainActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".LoginActivity" />
        <activity
            android:name=".SplashScreen"
            android:label="@string/app_name"
            android:noHistory="true"
            android:theme="@style/Theme.Design.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>